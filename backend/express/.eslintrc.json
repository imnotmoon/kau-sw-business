{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules" : {
      "prettier/prettier": "error",
      "no-unused-vars": "off"
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": false
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": ["prettier"]
  } 
      