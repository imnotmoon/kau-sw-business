{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules" : {
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"]
}